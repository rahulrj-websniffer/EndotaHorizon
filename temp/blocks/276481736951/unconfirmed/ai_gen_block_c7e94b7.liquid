{% doc %}
  @prompt
    Need slider hero banner
    Which can accomodate N number of images
    Need control over each slide
    
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-hero-slider-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    overflow: hidden;
    display: block;
  }

  .ai-hero-slider__viewport-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    height: {{ block.settings.slide_height_desktop }}px;
  }

  .ai-hero-slider__track-{{ ai_gen_id }} {
    display: flex;
    transition: transform 0.5s ease-in-out;
    height: 100%;
  }

  .ai-hero-slider__slide-{{ ai_gen_id }} {
    min-width: 100%;
    height: 100%;
    position: relative;
    display: flex;
    align-items: {{ block.settings.content_alignment_vertical }};
    justify-content: {{ block.settings.content_alignment_horizontal }};
  }

  .ai-hero-slider__image-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-hero-slider__image-placeholder-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-hero-slider__image-placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    max-width: 500px;
    max-height: 500px;
  }

  .ai-hero-slider__overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: {{ block.settings.overlay_color }};
    opacity: {{ block.settings.overlay_opacity | divided_by: 100.0 }};
  }

  .ai-hero-slider__content-{{ ai_gen_id }} {
    position: relative;
    z-index: 2;
    max-width: {{ block.settings.content_max_width }}px;
    padding: 40px;
    text-align: {{ block.settings.text_alignment }};
  }

  .ai-hero-slider__heading-{{ ai_gen_id }} {
    color: {{ block.settings.heading_color }};
    font-size: {{ block.settings.heading_size }}px;
    margin: 0 0 16px;
  }

  .ai-hero-slider__text-{{ ai_gen_id }} {
    color: {{ block.settings.text_color }};
    font-size: {{ block.settings.text_size }}px;
    margin: 0 0 24px;
  }

  .ai-hero-slider__button-{{ ai_gen_id }} {
    display: inline-block;
    padding: 12px 32px;
    background-color: {{ block.settings.button_background }};
    color: {{ block.settings.button_text_color }};
    text-decoration: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    transition: all 0.3s ease;
  }

  .ai-hero-slider__button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_background }};
    color: {{ block.settings.button_hover_text_color }};
  }

  .ai-hero-slider__controls-{{ ai_gen_id }} {
    position: absolute;
    bottom: 30px;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    gap: 12px;
    z-index: 3;
  }

  .ai-hero-slider__dot-{{ ai_gen_id }} {
    width: {{ block.settings.dot_size }}px;
    height: {{ block.settings.dot_size }}px;
    border-radius: 50%;
    background-color: {{ block.settings.dot_color }};
    opacity: 0.5;
    border: none;
    cursor: pointer;
    transition: opacity 0.3s ease, transform 0.3s ease;
    padding: 0;
  }

  .ai-hero-slider__dot-{{ ai_gen_id }}:hover {
    opacity: 0.8;
    transform: scale(1.1);
  }

  .ai-hero-slider__dot-{{ ai_gen_id }}.active {
    opacity: 1;
    transform: scale(1.2);
  }

  .ai-hero-slider__arrow-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    background-color: {{ block.settings.arrow_background }};
    color: {{ block.settings.arrow_color }};
    border: none;
    width: {{ block.settings.arrow_size }}px;
    height: {{ block.settings.arrow_size }}px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    z-index: 3;
    transition: all 0.3s ease;
    opacity: 0.8;
  }

  .ai-hero-slider__arrow-{{ ai_gen_id }}:hover {
    opacity: 1;
    transform: translateY(-50%) scale(1.1);
  }

  .ai-hero-slider__arrow-{{ ai_gen_id }}--prev {
    left: 20px;
  }

  .ai-hero-slider__arrow-{{ ai_gen_id }}--next {
    right: 20px;
  }

  .ai-hero-slider__arrow-{{ ai_gen_id }} svg {
    width: 24px;
    height: 24px;
  }

  @media screen and (max-width: 749px) {
    .ai-hero-slider__viewport-{{ ai_gen_id }} {
      height: {{ block.settings.slide_height_mobile }}px;
    }

    .ai-hero-slider__heading-{{ ai_gen_id }} {
      font-size: {{ block.settings.heading_size | times: 0.6 }}px;
    }

    .ai-hero-slider__text-{{ ai_gen_id }} {
      font-size: {{ block.settings.text_size | times: 0.9 }}px;
    }

    .ai-hero-slider__content-{{ ai_gen_id }} {
      padding: 20px;
    }

    .ai-hero-slider__arrow-{{ ai_gen_id }} {
      width: {{ block.settings.arrow_size | times: 0.8 }}px;
      height: {{ block.settings.arrow_size | times: 0.8 }}px;
    }

    .ai-hero-slider__arrow-{{ ai_gen_id }}--prev {
      left: 10px;
    }

    .ai-hero-slider__arrow-{{ ai_gen_id }}--next {
      right: 10px;
    }
  }
{% endstyle %}

<hero-slider-{{ ai_gen_id }} class="ai-hero-slider-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-hero-slider__viewport-{{ ai_gen_id }}">
    <div class="ai-hero-slider__track-{{ ai_gen_id }}">
      {% for i in (1..10) %}
        {% liquid
          assign image_key = 'slide_' | append: i | append: '_image'
          assign show_overlay_key = 'slide_' | append: i | append: '_show_overlay'
          assign heading_key = 'slide_' | append: i | append: '_heading'
          assign text_key = 'slide_' | append: i | append: '_text'
          assign button_label_key = 'slide_' | append: i | append: '_button_label'
          assign button_link_key = 'slide_' | append: i | append: '_button_link'

          assign slide_image = block.settings[image_key]
          assign slide_show_overlay = block.settings[show_overlay_key]
          assign slide_heading = block.settings[heading_key]
          assign slide_text = block.settings[text_key]
          assign slide_button_label = block.settings[button_label_key]
          assign slide_button_link = block.settings[button_link_key]
        %}

        {% if slide_image != blank or slide_heading != blank or slide_text != blank %}
          <div class="ai-hero-slider__slide-{{ ai_gen_id }}" data-slide-index="{{ forloop.index0 }}">
            {% if slide_image %}
              <img
                src="{{ slide_image | image_url: width: 2000 }}"
                alt="{{ slide_image.alt | escape }}"
                loading="lazy"
                width="{{ slide_image.width }}"
                height="{{ slide_image.height }}"
                class="ai-hero-slider__image-{{ ai_gen_id }}"
              >
            {% else %}
              <div class="ai-hero-slider__image-placeholder-{{ ai_gen_id }}">
                {{ 'hero-apparel-1' | placeholder_svg_tag }}
              </div>
            {% endif %}

            {% if slide_show_overlay %}
              <div class="ai-hero-slider__overlay-{{ ai_gen_id }}"></div>
            {% endif %}

            <div class="ai-hero-slider__content-{{ ai_gen_id }}">
              {% if slide_heading != blank %}
                <h2 class="ai-hero-slider__heading-{{ ai_gen_id }}">{{ slide_heading }}</h2>
              {% endif %}

              {% if slide_text != blank %}
                <div class="ai-hero-slider__text-{{ ai_gen_id }}">{{ slide_text }}</div>
              {% endif %}

              {% if slide_button_label != blank and slide_button_link != blank %}
                <a href="{{ slide_button_link }}" class="ai-hero-slider__button-{{ ai_gen_id }}">
                  {{ slide_button_label }}
                </a>
              {% endif %}
            </div>
          </div>
        {% endif %}
      {% endfor %}
    </div>
  </div>

  {% if block.settings.show_arrows %}
    <button class="ai-hero-slider__arrow-{{ ai_gen_id }} ai-hero-slider__arrow-{{ ai_gen_id }}--prev" aria-label="Previous slide">
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
        <polyline points="15 18 9 12 15 6"></polyline>
      </svg>
    </button>
    <button class="ai-hero-slider__arrow-{{ ai_gen_id }} ai-hero-slider__arrow-{{ ai_gen_id }}--next" aria-label="Next slide">
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
        <polyline points="9 18 15 12 9 6"></polyline>
      </svg>
    </button>
  {% endif %}

  {% if block.settings.show_dots %}
    <div class="ai-hero-slider__controls-{{ ai_gen_id }}">
      {% for i in (1..10) %}
        {% liquid
          assign image_key = 'slide_' | append: i | append: '_image'
          assign heading_key = 'slide_' | append: i | append: '_heading'
          assign text_key = 'slide_' | append: i | append: '_text'

          assign slide_image = block.settings[image_key]
          assign slide_heading = block.settings[heading_key]
          assign slide_text = block.settings[text_key]
        %}

        {% if slide_image != blank or slide_heading != blank or slide_text != blank %}
          <button
            class="ai-hero-slider__dot-{{ ai_gen_id }} {% if forloop.first %}active{% endif %}"
            data-slide="{{ forloop.index0 }}"
            aria-label="Go to slide {{ forloop.index }}"
          ></button>
        {% endif %}
      {% endfor %}
    </div>
  {% endif %}
</hero-slider-{{ ai_gen_id }}>

<script>
  (function() {
    class HeroSlider{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentSlide = 0;
        this.autoplayInterval = null;
      }

      connectedCallback() {
        this.track = this.querySelector('.ai-hero-slider__track-{{ ai_gen_id }}');
        this.slides = this.querySelectorAll('.ai-hero-slider__slide-{{ ai_gen_id }}');
        this.dots = this.querySelectorAll('.ai-hero-slider__dot-{{ ai_gen_id }}');
        this.prevButton = this.querySelector('.ai-hero-slider__arrow-{{ ai_gen_id }}--prev');
        this.nextButton = this.querySelector('.ai-hero-slider__arrow-{{ ai_gen_id }}--next');
        this.autoplay = {{ block.settings.autoplay }};
        this.autoplaySpeed = {{ block.settings.autoplay_speed }} * 1000;

        if (this.slides.length === 0) {
          return;
        }

        this.setupEventListeners();
        
        if (this.autoplay && this.slides.length > 1) {
          this.startAutoplay();
        }
      }

      setupEventListeners() {
        if (this.prevButton) {
          this.prevButton.addEventListener('click', () => this.goToPrevSlide());
        }

        if (this.nextButton) {
          this.nextButton.addEventListener('click', () => this.goToNextSlide());
        }

        this.dots.forEach((dot) => {
          dot.addEventListener('click', (e) => {
            const slideIndex = parseInt(e.target.dataset.slide);
            this.goToSlide(slideIndex);
          });
        });

        this.addEventListener('mouseenter', () => this.stopAutoplay());
        this.addEventListener('mouseleave', () => {
          if (this.autoplay && this.slides.length > 1) {
            this.startAutoplay();
          }
        });
      }

      goToSlide(index) {
        this.currentSlide = index;
        const offset = -100 * index;
        this.track.style.transform = `translateX(${offset}%)`;
        this.updateDots();
      }

      goToNextSlide() {
        this.currentSlide = (this.currentSlide + 1) % this.slides.length;
        this.goToSlide(this.currentSlide);
      }

      goToPrevSlide() {
        this.currentSlide = (this.currentSlide - 1 + this.slides.length) % this.slides.length;
        this.goToSlide(this.currentSlide);
      }

      updateDots() {
        this.dots.forEach((dot, index) => {
          if (index === this.currentSlide) {
            dot.classList.add('active');
          } else {
            dot.classList.remove('active');
          }
        });
      }

      startAutoplay() {
        this.stopAutoplay();
        this.autoplayInterval = setInterval(() => {
          this.goToNextSlide();
        }, this.autoplaySpeed);
      }

      stopAutoplay() {
        if (this.autoplayInterval) {
          clearInterval(this.autoplayInterval);
          this.autoplayInterval = null;
        }
      }

      disconnectedCallback() {
        this.stopAutoplay();
      }
    }

    customElements.define('hero-slider-{{ ai_gen_id }}', HeroSlider{{ ai_gen_id }});
  })();
</script>

